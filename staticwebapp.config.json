{
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "mimeTypes": {
    ".html": "text/html",
    ".css": "text/css",
    ".js": "text/javascript"
  },
  "globalHeaders": {
    "cache-control": "public, max-age=31536000, immutable"
  }
}
